	<skos:example xml:lang="en">821.111(540)-1    Indian English poetry</skos:example>
	<skos:example xml:lang="en">821.111(540)-3=112.2    Indian English fiction, (translated) in German</skos:example>
	<skos:example xml:lang="en">821.111`04    Mediaeval English literature</skos:example>
	<skos:example xml:lang="en">821.112.2`04-1    Mediaeval German poetry</skos:example>
	<skos:example xml:lang="en">821.133.1(091)    History of French literature</skos:example>
