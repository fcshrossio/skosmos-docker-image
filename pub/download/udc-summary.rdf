<rdf:Description rdf:about="http://udcdata.info/013087">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The -05 auxiliaries denote the persons concerned or their characteristics</skos:scopeNote>
	<skos:example xml:lang="en">324-05    Persons connected with elections</skos:example>
	<skos:example xml:lang="en">324-051    Electors</skos:example>
	<skos:example xml:lang="en">324-052    Elected representatives</skos:example>
	<skos:example xml:lang="en">324-052-055.2    Female elected representatives</skos:example>
	<skos:example xml:lang="pt">324-05 Pessoas com funções eleitorais</skos:example>
