<rdf:Description rdf:about="http://udcdata.info/000002">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:scopeNote xml:lang="en">The coordination sign + (plus) connects two or more separated (non-consecutive, non-related) UDC numbers, to denote a compound subject for which no single number exists</skos:scopeNote>
	<skos:example xml:lang="pt">(44+460) França e Espanha</skos:example>
	<skos:example xml:lang="pt">(470+571) Rússia. Federação Russa</skos:example>
	<skos:example xml:lang="pt">622+669 Mineração e metalurgia</skos:example>
	<skos:example xml:lang="en">(44+460) France and Spain</skos:example>
	<skos:example xml:lang="en">(470+571) Russia. Russian federation</skos:example>
	<skos:example xml:lang="en">622+669 Mining and Metallurgy</skos:example>
	<skos:inScheme rdf:resource="http://udcdata.info/udc-schema"/>
	<skos:prefLabel xml:lang="en">Coordination. Addition (plus sign). Table 1a</skos:prefLabel>
	<skos:prefLabel xml:lang="pt">Coordenação. Adição (sinal de mais). Tabela 1a</skos:prefLabel>
	<skos:notation rdf:datatype="http://udcdata.info/UDCnotation">+</skos:notation>
	<skos:note xml:lang="pt">O sinal de coordenação + (mais) liga dois ou mais números separados (não consecutivos), para indicar um assunto composto para o qual não existe um número simples</skos:note>
	<skos:broader rdf:resource="http://udcdata.info/078885"/>
</rdf:Description>

